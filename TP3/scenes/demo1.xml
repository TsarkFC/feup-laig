    <lights>
        <light id="defaultLight1">            
            <enable value="1" />
            <position x="0" y="-20" z="-12" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />                   
            <diffuse r="0.8" g="0.8" b="0.8" a="1.0" />            
            <specular r="0.8" g="0.8" b="0.8" a="1.0" />            
        </light>
        <light id="defaultLight2">            
