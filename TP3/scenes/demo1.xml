            <specular r="0.8" g="0.8" b="0.8" a="1.0" />            
        </light>
        <light id="dragonLight">            
            <enable value="1" />
            <position x="0" y="4" z="0" w="0.9" />
            <ambient r="0" g="0" b="0" a="1.0" />        
            <diffuse r="1" g="0.05" b="0.05" a="1.0" />            
