            <texture id="null" />

            <transformations>
                <scale sx="1.6" sy="1.6" sz="1" />
                <rotation axis="x" angle="-90" />
            </transformations>

            <descendants>
                <leaf type="torus" inner="2.1" outer="15" slices="32" loops="128" /> 
            </descendants>
        </node>
        <!--
            ********** QUIDDITCH HOOPS **********
        -->
        <node id="hoopSetL">
