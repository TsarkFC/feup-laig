            </transformations>

            <descendants>
                <leaf type="cylinder" height="10" topRadius="0.5" bottomRadius="0.5" stacks="5" slices="20"/>
            </descendants>
        </node>
        <node id="hoopHole">
            <material id="null" />

            <texture id="null" />

            <descendants>
