            </transformations>

            <descendants>
                <noderef id="hoop1" />
                <noderef id="hoop2" />
                <noderef id="hoop3" />
            </descendants>
        </node>
        <node id="hoop1">
            <material id="null" />

            <texture id="null" />

            <descendants>
                <noderef id="hoopPole1" />
