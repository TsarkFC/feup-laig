            <descendants>
                <leaf type="triangle" x1="0" y1="0" x2="3" y2="0" x3="3" y3="5" />
            </descendants>
        </node>
        <node id="parallelogramTriangle2">
            <material id="null" />

            <texture id="null">
                <amplification afs="15" aft="15" />
