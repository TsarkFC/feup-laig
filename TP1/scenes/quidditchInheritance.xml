                <rotation axis="y" angle="-90" />
            </transformations>

            <descendants>
                <noderef id="tentWall" />
            </descendants>
        </node>
        <node id="tentWall">
            <material id="null" />

            <texture id="null">
                <amplification afs="8" aft="8" />
            </texture>

            <descendants>
                <leaf type="rectangle" x1="0" y1="0" x2="8" y2="5" />
            </descendants>
        </node>

        <!--
            *************************************
            ********** QUIDDITCH FIELD **********
            *************************************
        -->
        <node id="quidditchField">
            <material id="null" />

            <texture id="null" />

            <descendants>
                <noderef id="fieldPlayingArea" />
                <noderef id="fieldDelimiters" />
            </descendants>
        </node>
        <node id="fieldPlayingArea">
            <material id="null" />

            <texture id="null" />

            <descendants>
                <noderef id="hoopSetL" />
                <noderef id="hoopSetR" />
            </descendants>
        </node>
        <node id="fieldDelimiters">
            <material id="null" />

            <texture id="null" />

            <descendants>
                <noderef id="fieldGroundBarrier" />
