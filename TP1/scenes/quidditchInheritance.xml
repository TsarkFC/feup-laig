        </node>
        <node id="hoop3">
            <material id="null" />

            <texture id="null" />

            <transformations>
                <translation x="0" y="0" z="-3.5" />
            </transformations>

            <descendants>
                <noderef id="hoopPole3" />
                <noderef id="hoopHole3" />
            </descendants>
        </node>
        <node id="hoopHole3">
            <material id="null" />

            <texture id="null" />

