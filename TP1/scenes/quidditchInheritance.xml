            </texture>

            <descendants>
                <leaf type="triangle" x1="-4" y1="0" x2="4" y2="0" x3="0" y3="4.2426" /> <!-- sqrt(18) => sqrt(34)**2 = 4**2 + x**2-->
            </descendants>
        </node>
        <node id="tentParallelogram1">
            <material id="null" />

            <texture id="null" />

            <transformations>
                <translation x="8" y="5" z="20" />
