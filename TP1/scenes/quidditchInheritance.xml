            <transformations>
                <translation x="0" y="6.95" z="0" />
                <rotation axis="x" angle="-90" />
            </transformations>

            <descendants>
                <leaf type="sphere" radius="0.2" slices="15" stacks="15" />
            </descendants>
        </node>
        <node id="towerCeilingPole">
            <material id="null" />

            <texture id="null" />

            <transformations>
