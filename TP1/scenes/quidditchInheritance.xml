        </node>
        <node id="tentParallelogram2">
            <material id="null" />

            <texture id="null" />

            <transformations>
                <translation x="0" y="5" z="0" />
                <rotation axis="z" angle="-53.13" /> <!-- 90 - arctan(3/4) -->
                <rotation axis="y" angle="270" />
            </transformations>

            <descendants>
                <noderef id="tentParallelogram" />
            </descendants>
        </node>
        <node id="tentParallelogram">
            <material id="null" />

            <texture id="null" />

            <descendants>
                <noderef id="parallelogramTriangle1" />
                <noderef id="parallelogramTriangle2" />
